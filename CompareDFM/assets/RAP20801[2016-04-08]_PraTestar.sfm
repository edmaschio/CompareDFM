[TwwQuery]
AutoCalcFields=False
DataBaseName=DBObj
Name=QRAT002
Params
   idpadrao,Integer,Input
   idempresa,Integer,Input
   identpadrao,String,Input
   codpadrao,Integer,Input
   idcontrato,Integer,Input
Sql
   DECLARE
   @idpadrao     INT,
   @idempresa    INT,
   @identpadrao  VARCHAR(10),
   @codpadrao    INT,
   @idcontrato   INT,
   @datarecisao  DATETIME
   
   SET @idpadrao     = :idpadrao
   SET @idempresa    = :idempresa
   SET @identpadrao  = :identpadrao
   SET @codpadrao    = :codpadrao
   SET @idcontrato   = :idcontrato
   
   SELECT @datarecisao = a029.datadesliga
   FROM RAT01000 A010
   LEFT JOIN RAT02900 A029 ON A029.iddesliga = a010.idcontrato
   WHERE A010.idcontrato = @idcontrato
   
   SELECT A002.*, A003.valor,
   A003.anoreferenc ,A003.mesreferenc
   INTO #temp1
   FROM RAT00200 A002
   LEFT JOIN RAT00300 A003 ON A003.idpadrao = A002.idpadrao AND
   CONVERT(DATETIME, CONVERT(CHAR,A003.anoreferenc) + '-' +
   CONVERT(CHAR,A003.mesreferenc) + '-1') =
   (SELECT MAX(CONVERT(DATETIME, CONVERT(CHAR,A003a.anoreferenc) + '-' +
   CONVERT(CHAR,A003a.mesreferenc) + '-1'))
   FROM RAT00300 A003a
   WHERE A003a.idpadrao = A002.idpadrao )
   WHERE
   a002.idpadrao    = ISNULL(@idpadrao,a002.idpadrao)       AND
   a002.identpadrao = ISNULL(@identpadrao,a002.identpadrao) AND
   a002.codpadrao   = ISNULL(@codpadrao,a002.codpadrao)     AND
   a002.idempresa   = ISNULL(@idempresa,a002.idempresa)     AND
   @datarecisao IS NULL
   
   UNION
   
   SELECT A002.*, A003.valor,
   A003.anoreferenc ,A003.mesreferenc
   FROM RAT00200 A002
   LEFT JOIN RAT00300 A003 ON A003.idpadrao = A002.idpadrao
   WHERE
   a002.idpadrao    = ISNULL(@idpadrao,a002.idpadrao)       AND
   a002.identpadrao = ISNULL(@identpadrao,a002.identpadrao) AND
   a002.codpadrao   = ISNULL(@codpadrao,a002.codpadrao)     AND
   a002.idempresa   = ISNULL(@idempresa,a002.idempresa)     AND
   CONVERT(DATETIME, CONVERT(VARCHAR(4),A003.anoreferenc) + '-' +
   CONVERT(VARCHAR(2),A003.mesreferenc) + '-1') <= @datarecisao
   
   SELECT TOP 1 * FROM #temp1
   ORDER BY anoreferenc DESC ,mesreferenc DESC
   DROP TABLE #temp1

