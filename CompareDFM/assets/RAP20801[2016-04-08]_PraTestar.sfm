[TwwQuery]
AutoCalcFields=False
DataBaseName=DBObj
Name=qDataFimExerc
Params
   datainiexerc,DateTime,Input
   datafimexerc,DateTime,Input
   dias,Integer,Input
Sql
   Declare
     @dini datetime,
     @fim  datetime,
     @dias int
   
   set @dini = :datainiexerc
   set @fim  = :datafimexerc
   set @dias = :dias
   
   select dias        = CASE
                          WHEN @dias > 0 then @dias
                          ELSE ISNULL(datediff(day,@dini,@fim)+1,0)
                        END,
          datainicio  = @dini,
          datatermino = CASE
                          WHEN @dias > 0 THEN DATEADD(DAY, @dias, @dini) - 1
                          ELSE @fim
                        END

