[TwwQuery]
AutoCalcFields=False
DataBaseName=DBObj
Name=Qdata_adm
Params
   data,DateTime,Input
Sql
   DECLARE
     @data              DATETIME,
     @dia_semana        CHAR (3),
     @EhFeriadoProducao CHAR (1)
   
   SET @data = :data
   
   SELECT @dia_semana        = DiaDaSemanaCalendario,
          @EhFeriadoProducao = EhFeriadoProducao
   FROM XXT00600
   WHERE
     DtCalendario = @data
   
   IF ((@dia_semana = 'Sab') OR (@dia_semana = 'Dom') OR (@EhFeriadoProducao = 'S'))
     WHILE ((@dia_semana = 'Sab') OR (@dia_semana = 'Dom') OR (@EhFeriadoProducao = 'S'))
     BEGIN
       SET @data = DATEADD(DAY, 1, @data)
   
       SELECT @dia_semana        = DiaDaSemanaCalendario,
              @EhFeriadoProducao = EhFeriadoProducao
       FROM XXT00600
       WHERE
         DtCalendario = @data
     END
   
   SELECT prox_dia_util = @data

