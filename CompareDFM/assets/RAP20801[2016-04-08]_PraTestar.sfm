[TwwQuery]
AutoCalcFields=False
DataBaseName=DBObj
Name=QAgNocivoVin
Params
   idfuncionario,Integer,Input
   regimejur,Integer,Input
Sql
   DECLARE @idfuncionario INT,
           @regimejur     INT,
           @relatorio     VARCHAR(8)
   
   SET @idfuncionario = :idfuncionario
   SET @regimejur     = :regimejur
   SET @relatorio     = 'AGNOCVIN'
   
   SELECT a009.matricfunci, vinculos = COUNT(*)
   FROM RAT01000 A010
   INNER JOIN RFT03801 F038 ON F038.idtabdiv      = A010.regimejur
   INNER JOIN RFT03700 F037 ON F037.idfolsintcol  = F038.idfolsintcol
                           AND F037.nrocoluna     = 1
   INNER JOIN RFT04000 F040 ON F040.idtitulo      = F037.idtitulo
                           AND F040.nomerelatorio = @relatorio
   INNER JOIN RAT00900 A009 ON A009.idfuncionario = A010.idfuncionario
   LEFT  JOIN RXT00100 X001 ON X001.idtabdiv      = A010.agentnociv
                           AND X001.codtabdiv     = 25
   WHERE A010.idfuncionario    = @idfuncionario
     AND A010.rescisao         = 0
     AND A010.semprocessamento < 2
     AND EXISTS (SELECT 1 FROM XXT00900 WHERE TpParamSis = 'SMARrh' AND NomeParamSis = 'RAP20801-AgNocivoMultiVinculo' AND ValorParamSis = 'S')
     AND EXISTS (SELECT 1 FROM RFT03801 F038a WHERE idtabdiv = @regimejur AND idfolsintcol = F037.idfolsintcol)
   GROUP BY A010.idfuncionario, a009.matricfunci

